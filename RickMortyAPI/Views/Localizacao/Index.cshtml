@model List<LocalizacaoViewModel>

<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <title>Localizacao</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>
    <div class="container">
        <h1>Rick and Morty Localizacoes</h1>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Nome</th>
                    <th>Tipo</th>
                    <th>Dimensão</th>
                    <th>Criado</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var localizacao in Model)
                {
                    <tr>
                        <td>@localizacao.Nome</td>
                        <td>@localizacao.Tipo</td>
                        <td>@localizacao.Dimensao</td>
                        <td>@localizacao.Criado</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
